    <cx:subrule rdf:resource="http://celtx.com/NS/v1/DocRule/description"/>
    <cx:subrule>
      <cx:DocRule cx:arcname="http://celtx.com/NS/v1/filter"
                  cx:arctype="Resource"
                  cx:unique="true"
                  cx:owner="true">
        <cx:subrule rdf:resource="http://celtx.com/NS/v1/DocRule/rdftype"/>
        <cx:subrule>
          <cx:DocRule cx:arcname="http://www.w3.org/1999/02/22-rdf-syntax-ns#li"
                      cx:arctype="Resource"
                      cx:unique="false"
                      cx:owner="false"/>
        </cx:subrule>
      </cx:DocRule>
    </cx:subrule>
    <cx:subrule>
      <cx:DocRule cx:arcname="http://celtx.com/NS/v1/members"
                  cx:arctype="Resource"
                  cx:unique="true"
                  cx:owner="true">
        <cx:subrule rdf:resource="http://celtx.com/NS/v1/rdftype"/>
        <cx:subrule>
          <cx:DocRule cx:arcname="http://www.w3.org/1999/02/22-rdf-syntax-ns#li"
                      cx:arctype="Resource"
                      cx:unique="false"
                      cx:owner="false"/>
        </cx:subrule>
      </cx:DocRule>
    </cx:subrule>
    <cx:subrule>
      <cx:DocRule cx:arcname="http://celtx.com/NS/v1/departments"
                  cx:arctype="Resource"
                  cx:unique="true"
                  cx:owner="true">
        <cx:subrule rdf:resource="http://celtx.com/NS/v1/rdftype"/>
        <cx:subrule>
          <cx:DocRule cx:arcname="http://www.w3.org/1999/02/22-rdf-syntax-ns#li"
                      cx:arctype="Resource"
                      cx:unique="false"
                      cx:owner="true">
            <cx:subrule rdf:resource="http://celtx.com/NS/v1/DocRule/rdftype"/>
            <cx:subrule>
              <cx:DocRule cx:arcname="http://celtx.com/NS/v1/department"
                          cx:arctype="Resource"
                          cx:unique="true"
                          cx:owner="false"/>
            </cx:subrule>
            <cx:subrule>
              <cx:DocRule cx:arcname="http://www.w3.org/1999/02/22-rdf-syntax-ns#li"
                          cx:arctype="Resource"
                          cx:unique="false"
                          cx:owner="true"/>
            </cx:subrule>
          </cx:DocRule>
        </cx:subrule>
      </cx:DocRule>
    </cx:subrule>
  </cx:DocRule>
        

